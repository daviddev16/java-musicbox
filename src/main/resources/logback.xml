<configuration>

 	 <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
 	   <encoder class="ch.qos.logback.classic.encoder.PatternLayoutEncoder">
    	  <pattern>%d{yyyy-MM-dd HH:mm:ss} [%thread] [ %-5level] %class{0} -> %msg%n</pattern>
   	 </encoder>
 	 </appender>
 	 <root level="INFO">
  	  <appender-ref ref="CONSOLE"/>
  	</root>

	<logger name="com.sedmelluq.lava.common.natives.NativeLibraryLoader" level="OFF"/>
	<logger name="com.sedmelluq.discord.lavaplayer.source.youtube.format.StreamingDataFormatsExtractor" level="OFF"/>
	<logger name="com.sedmelluq.lava.common.natives.ResourceNativeLibraryBinaryProvider" level="OFF"/>
</configuration>